{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "bugfixes": true,
        "shippedProposals": true
      }
    ],
    // ["@babel/react", { "runtime": "automatic" }], // for dev
    ["@babel/react"], // for build
    "@babel/typescript",
    "@linaria"
  ],
  "plugins": [
    "@babel/transform-runtime",
    ["optimize-clsx", { "functionNames": ["getCellClassname"] }]
  ]
}
